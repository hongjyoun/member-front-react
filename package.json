  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "@apps/*",
      "@packages/*"
    ]
  },
  "scripts": {
    "init": "npm run clear && npm i",
    "clear": "bash ./clear.sh",
    "admin:start": "npm run start -w @member/admin",
